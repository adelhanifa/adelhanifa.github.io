<mainid="main">

    <!-- ======= Breadcrumbs ======= -->
    <div class="breadcrumbs" data-aos="fade-in">
        <div class="container">
            <h2>Log In</h2>
            <p>Please enter your email & password to log in to your Profile</p>
        </div>
    </div><!-- End Breadcrumbs -->


    <!-- ======= Contact Section ======= -->
    <section class="my-5">
        <div class="alert alert-danger container w-50 {{notMatch}}" role="alert">
            The email or password do not match, please check your inputs again....!
        </div>
        <div class="container w-50 my-4">
            <form action="/log-in" method="POST">
                <div class="form-group">
                    <label>Email address</label>
                    <input type="email" class="form-control" name="email">
                </div>
                <div class="form-group">
                    <label>Password</label>
                    <input type="password" class="form-control" name="password">
                </div>
                <div class="form-group">
                    <label class="form-check-label">If you don't have an account <a href="/sign-up">create
                            one</a></label>
                    <button type="submit" class="get-started-btn float-right ">Sign in</button>
                </div>
            </form>
        </div>

    </section><!-- End Contact Section -->

    </main><!-- End #main -->